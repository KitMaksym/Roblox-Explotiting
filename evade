local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("evade: kit_maksym", "DarkTheme")
local Tab = Window:NewTab("player")
local Section1 = Tab:NewSection("player")
Section1:NewKeybind("Revive", "just respawn", Enum.KeyCode.R, function()
	game:GetService("ReplicatedStorage").Events.Respawn:FireServer()
end)
Section1:NewButton("inf yield", "ButtonInfo", function()
    print("Clicked")
end)

local Tab = Window:NewTab("esp")
local Section2 = Tab:NewSection("esp")
Section1:NewButton("monster esp", "uh", function()
    print("Clicked")
end)
local Tab = Window:NewTab("menu")
local Section3 = Tab:NewSection("menu")
Section3:NewKeybind("put all tabs in one", "bruh who reading this if you know hide ui button", Enum.KeyCode.L, function()
	Library:ToggleUI()
end)
